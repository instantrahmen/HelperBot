{
  "name": "@helper/common",
  "version": "0.2.0",
  "author": "erika_tho",
  "module": "dist/index.mjs",
  "size-limit": [
    {
      "path": "dist/common.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/common.esm.js",
      "limit": "10 KB"
    }
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start:tsdx": "tsdx watch",
    "build:tsdx": "tsdx build",
    "prepare:tsdx": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean": "rimraf dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "dependencies": {
    "config": "workspace:0.1.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.9.0",
    "@types/yaml": "^1.9.7",
    "js-yaml": "^4.1.0",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "pm2": "^5.2.0",
    "pnpm": "^7.2.0",
    "rimraf": "^5.0.5",
    "tsdx": "^0.14.1",
    "vite": "^5.2.6",
    "vite-plugin-dts": "^3.8.1",
    "vite-plugin-node": "^3.1.0"
  }
}
